{
  "name": "cocktail-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "jest --passWithNoTests",
    "test:changed": "node scripts/test-changed.js"
  },
  "dependencies": {
    "@vercel/node": "^3.0.0",
    "html2pdf.js": "^0.10.3",
    "node-fetch": "^3.3.2",
    "pinia": "^2.3.1",
    "vue": "^3.4.0",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/sass": "^1.43.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/vue3-jest": "^29.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.0",
    "sass": "^1.85.1",
    "sass-embedded": "^1.85.1",
    "sass-loader": "^16.0.5",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "type": "module"
}
