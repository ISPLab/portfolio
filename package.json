{
  "name": "portfolio-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",   
    "dev": "vite",
    "dev:backend": "cd backend && npm run start:dev",
    "dev:all": "concurrently \"cd frontend && npm run dev\" \"cd backend && npm run start:debug\"",
    "install:all": "npm install && cd backend && npm install",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:push": "chmod +x scripts/docker-push.sh && ./scripts/docker-push.sh",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@vercel/node": "^3.0.0",
    "axios": "^1.6.7",
    "serve": "^14.0.0",
    "html2pdf.js": "^0.10.3",
    "node-fetch": "^3.3.2",
    "pinia": "^2.3.1",
    "vue": "^3.4.0",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/sass": "^1.43.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/vue3-jest": "^29.0.0",
    "concurrently": "^8.2.2",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.0",
    "sass": "^1.85.1",
    "sass-embedded": "^1.85.1",
    "sass-loader": "^16.0.5",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "type": "module"
}
